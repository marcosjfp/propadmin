{
  "name": "administrador-de-propriedades",
  "version": "1.0.0",
  "description": "Aplicação de gerenciamento de propriedades",
  "main": "server/_core/index.ts",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "pnpm exec tsx watch server/_core/index.ts",
    "start": "npx tsx server/_core/index.ts",
    "build": "cd client && npm install && npm run build",
    "postinstall": "npm run build",
    "client-dev": "pnpm --dir client dev",
    "client-build": "pnpm --dir client build",
    "type-check": "tsc --noEmit",
    "db:push": "pnpm exec drizzle-kit push",
    "db:migrate": "pnpm exec drizzle-kit migrate",
    "db:studio": "pnpm exec drizzle-kit studio"
  },
  "keywords": [
    "propriedades",
    "gerenciamento",
    "imobiliário"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@trpc/server": "^10.45.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.31.2",
    "express": "^4.18.2",
    "mysql2": "^3.9.0",
    "superjson": "^2.2.1",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.14"
  }
}
